var randomScalingFactor=function(){return Math.round(100*Math.random())},ctx=document.getElementById("myChart").getContext("2d"),thedoe=new Chart(ctx,{type:"doughnut",data:{labels:["1","2"],datasets:[{label:"Fscriptreaders",data:[randomScalingFactor(),100],backgroundColor:["rgba(255, 255, 255, 0.5)","rgba(255, 255, 255, 0.2)"],borderWidth:0,cutout:150,borderRadius:0,circumference:360,rotation:0}]},options:{labels:{display:!1},xAxes:{display:!1},yAxes:{display:!1},tooltip:{enabled:!1},gridLines:{display:!1},plugins:{legend:{display:!1},tooltip:{enabled:!1}}}});window.onload=function(){var a=document.getElementById("myChart").getContext("2d");window.thedoe=new Chart(a)},document.getElementById("randomizeData").addEventListener("click",function(){thedoe.data.datasets.forEach(function(a){a.data=a.data.map(function(){return randomScalingFactor()})}),window.thedoe.update()})
